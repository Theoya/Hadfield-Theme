{
    "name": "Hadfield Dark",
    "type": "dark",
    "colors": {
        "activityBar.background": "#2A2520",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#666666",
        "activityBarBadge.background": "#007acc",
        "activityBarBadge.foreground": "#ffffff",
        "editor.background": "#2A2520",
        "editor.foreground": "#d4d4d4",
        "editor.lineHighlightBackground": "#3A3530",
        "editor.selectionBackground": "#264f78",
        "editorCursor.foreground": "#ffffff",
        "sideBar.background": "#2A2520",
        "sideBar.foreground": "#cccccc",
        "sideBarTitle.foreground": "#cccccc",
        "statusBar.background": "#007acc",
        "statusBar.foreground": "#ffffff",
        "titleBar.activeBackground": "#3A3530",
        "titleBar.activeForeground": "#cccccc"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#FFA500"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": {
                "foreground": "#B57EDC"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": {
                "foreground": "#00FFFF"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Types",
            "scope": ["entity.name.type", "support.type"],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.class", "support.class"],
            "settings": {
                "foreground": "#B19CD9"
            }
        },
        {
            "name": "Structs and Objects",
            "scope": ["entity.name.struct", "entity.name.type", "entity.name.type.interface"],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Lists and Collections",
            "scope": [
                "entity.name.type.array",
                "entity.name.type.list",
                "entity.name.type.collection",
                "entity.name.type.dictionary",
                "entity.name.type.map",
                "entity.name.type.set"
            ],
            "settings": {
                "foreground": "#D8BFD8"
            }
        },
        {
            "name": "Braces (Curly)",
            "scope": ["punctuation.definition.block", "punctuation.section.block.begin", "punctuation.section.block.end"],
            "settings": {
                "foreground": "#89CFF0"
            }
        },
        {
            "name": "Braces (Round/Square)",
            "scope": ["punctuation.section.parens.begin", "punctuation.section.parens.end", "punctuation.section.brackets.begin", "punctuation.section.brackets.end"],
            "settings": {
                "foreground": "#B2FBA5"
            }
        }
    ]
} 